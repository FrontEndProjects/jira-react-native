    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-background-timer": "^1.0.3",
    "react-native-datepicker": "^1.4.2",
    "react-native-fcm": "^6.0.3",
    "react-native-linear-gradient": "^2.0.0",
    "react-native-loading-spinner-overlay": "^0.4.1",
    "react-native-localization": "^0.1.30",
